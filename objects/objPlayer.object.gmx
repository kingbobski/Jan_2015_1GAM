            <string>//FPS debug

if(keyboard_check_pressed(vk_f1))
{
    room_speed = 30;
}
if(keyboard_check_pressed(vk_f2))
{
    room_speed = 60;
}

// KEYBOARD
if(keyboard_check_direct(vk_right) || keyboard_check(ord('D')))
{
    motion_set(0,3);
}

if(keyboard_check(vk_left) || keyboard_check(ord('A')))
{
    motion_set(180,3);
}

if(keyboard_check(vk_up) || keyboard_check(ord('W')))
{
    motion_set(90,3);
}

if(keyboard_check(vk_down) || keyboard_check(ord('S')))
{
    motion_set(270,3);
}

if(keyboard_check(vk_nokey))
{
    speed = 0;
}

//CANNON POSITION
if(direction = 0)
{
    CannonPosX = sprite_width;
    CannonPosY = 20;
}
if(direction = 180)
{
    CannonPosX = 0;
    CannonPosY = 20;
}


// BULLETS
if(keyboard_check_pressed(vk_space))
{
    
    bullet = instance_create(x + CannonPosX,y + CannonPosY,objBullet);    

    if(direction = 0)
    {
        bullet.direction = 0;
        bullet.speed = 6;
    }
    if(direction = 180)
    {
        bullet.direction = 180;
        bullet.speed = 6;
    }
    // PLACEHOLDER
    if(direction = 90)
    {
        bullet.direction = 180;
        bullet.speed = 6;
    }
    //ANOTHER PLACEHOLDER
    if(direction = 270)
    {
        bullet.direction = 0;
        bullet.speed = 6;
    }
}
</string>
